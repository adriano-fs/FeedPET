<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.feedpet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@drawable/pet_logo"
        android:label="@string/app_name" >
        <activity android:name="com.feedpet.ShowNotice" />
        <activity android:name="com.feedpet.eventoDaBusca" />
        <activity android:name="com.feedpet.Noticias" />
        <activity android:name="com.feedpet.HomeSobre" />
        <activity android:name="com.feedpet.NoticiasGeral" />
        <activity android:name="com.feedpet.RSSContentActivity" />
        <activity
            android:name="com.feedpet.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
        <meta-data android:value="@string/app_id" android:name="com.facebook.sdk.ApplicationId"/>
        
        <activity android:name="com.facebook.LoginActivity"></activity>
        -->

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>